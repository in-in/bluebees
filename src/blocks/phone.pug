mixin phone(nums)
  mj-text(
    align='right'
    font-size='14px'
    color=whiteColor)
    img(
      style='padding-left: 10px; padding-top: 6px;'
      alt=''
      height='auto'
      width='17'
      src='assets/images/icon-phone.png'
      align='right')
    each n in nums
      p.phone!= n
